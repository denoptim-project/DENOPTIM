# Initial candidates converted from existing molecules
GA-InitMolsToFragmentFile=initial_pop.sdf
FRG-CuttingRulesFile=cutting_rules
#GA-InitPoplnFile=initial_pop.sdf

# Definition of the Fragment Space
FS-scaffoldLibFile=lib_scaff.sdf
FS-fragmentLibFile=lib_frags.sdf
FS-cappingFragmentLibFile=lib_cap.sdf
FS-compMatrixFile=CPMap.par
FS-rcCompMatrixFile=RC-CPMap.par
FS-MaxMW=2000

# Control ring formation
RC-CLOSERINGS

# Genetic Algorithm parameters
GA-randomSeed=8572368003836
GA-Parallelization=synchronous
GA-numParallelTasks=4
GA-populationSize=10
GA-numChildren=10
GA-numGenerations=10
GA-numConvGen=9
GA-XOverSelectionMode=TS
GA-crossoverWeight=1.0
GA-mutationWeight=1.0
GA-ConstructionWeight=0
GA-symmetryProbability=0.5
GA-LevelGrowthProbScheme=sigma
GA-LevelGrowthSigmaSteepness=1.0
GA-LevelGrowthSigmaMiddle=1.0
GA-EXCLUDEMUTATIONTYPE=EXTEND

FP-equation=${MW}
FP-CheckUidBeforeFitness=yes
