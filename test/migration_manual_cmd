f=data/lib_cap.sdf ; sed "s|FFFFFF|$f|" f.par > ff.par ; java -jar /Users/marco/tools/DENOPTIM_graphTemplate/build/MigrateV2ToV3.jar ff.par

mv ${f} ${f}_originalV2 ; mv ${f}_migratedV3 ${f}



