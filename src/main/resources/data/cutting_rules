#OOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOO
#OOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOO
#
# Alkyl ESTERS + ACIDS
# keep the geometry of O=C-O by cutting the sigma bond C-C=O instead of C-O,
# R3C-|-C(=O)O
# S: R3 = H, H, R or H
# M: R3 = H, R, R
# L: R3 = R, R, R
# ignore C=C-C(=O)O and Ph-C(=O)O
CTR acEstS 1200 [$(C(=O)[O;X2][#6]);!$(C(=O)OC=O)] [$([C;X4]([#1])([#1])C(=O)O),$([#1])] -!@
CTR acEstM 1210 [$(C(=O)[O;X2][#6]);!$(C(=O)OC=O)] [$(C([#1])C(=O)O);!$(C=C);!c;X4] -!@
CTR acEstL 1220 [$(C(=O)[O;X2][#6]);!$(C(=O)OC=O)] [$(CC(=O)O);!c;X4] -!@
#
################################################
# Carbamate
# cut R-|-O-C(=O)-NR2
CTR carbamateSubO 1250 [O;X2;$(OC(=O)N([#1,#6])[#1,#6])] [$([#6]-O-C(=O)-N)] -!@
#
################################################
# Carbonate
# cut R-|-O-C(=O)-O-R
CTR carbonateSubO 1260 [O;X2;$(OC(=O)O[#6])] [$([#6]-O-C(=O)-O)] -!@
#
################################################
# Alkyl ALCOHOLS
# cut alcohols
# ignore Ph-OH, enole =C-OH
# S: X3 = H, H, R
# L: X3 = R, R, R
CTR alcohS 1300 [O;X2;$(O[#1])] [$(CO);$(C([#1])([#1]));!$(C=O);X4] -!@
CTR alcohL 1310 [O;X2;$(O[#1])] [$(CO);!$(C=O);!c;X4] -!@
# cut esterified alkylic alcohols X3C-|-O-C(=O)
CTR alcohES 1320 [O;X2;$(OC(=O)[#6])] [$(CO);$(C([#1])([#1]));!$(C=O);X4] -!@
CTR alcohEL 1330 [O;X2;$(OC(=O)[#6])] [$(CO);!$(C=O);!c;X4] -!@
#
################################################
# Enol ETHERS
# cut R-|-O-C=C with R alkyl
# ignore R-O-Ph and Ph-O-C=C
CTR EnolEthR 1400 [O;X2;$(O-C=C);$(O-[C;X4]);!$(OC=O);!$(Oc)] [C;X4;$(C-O-C=C)] -!@
#
# cut Ar-|-O-C=C
CTR EnolEthA 1410 [O;X2;$(O-C=C);$(Oc);!$(OC=O)] [$(c-O-C=C)] -!@
#
################################################
# ETHERS
# cut R-|-O-R
# ignore acetals and emiacetals
CTR EthRR 1450 [O;!$(O[#1]);!$(OC=O);$(O([C;X4])[C;X4]);!$(O[C;X4]O[C;X4]);!$(O[C;X4]O[#6;X3])] [C;X4;$(C-O-[C;X4]);!$(C(OC)OC);!$(C(OC)O[#1])] -!@
#
# cut Ar-|-O-R
# ignore acetals
CTR EthArR 1460 [$(Oc);!$(OC=O);!$(O[C;X4]O[C;X4]);!$(O[C;X4]O[#6;X3])] [$(c-O-[C;X4])] -!@
#
# cut R-|-O-Ar
# ignore acetals
# ignore Ar-OMe
CTR EthRAr 1470 [$(Oc);!$(OC=O);!$(O[C;X4]O[C;X4]);!$(O[C;X4]O[#6;X3])] [$([C;X4]-O-c);!$([C;X4]([#1])([#1])([#1])-O-c)] -!@
#
# cut Ar-|-O-Ar
# ignore acetals
CTR EthArAr 1480 [$(Oc);!$(OC=O);!$(O[C;X4]O[C;X4]);!$(O[C;X4]O[#6;X3])] [$(c-O-c)] -!@
#
################################################
# CARBONYL on alkyl
# cut X3C-|-C(=O)-R/Ar
# S: X3 = H, H, R
# L: X3 = R, R, R
CTR ketonS 1500 [C;X3;$(C(=O)([C,c])[C,c])] [$([C;X4]([#1])([#1]))] -!@
CTR ketABI 1510 [C;X3;$(C(=O)([C,c])[C,c])] [$([C;X3]=[C;X3])] -!@
CTR ketonL 1520 [C;X3;$(C(=O)([C,c])[C,c])] [$([C;X4])] -!@
#
# cuts X3C-|-C(=O)H
CTR aldehS 1550 [C;X3;$(C(=O)[#1])] [$([C;X4]([#1])([#1]))] -!@
CTR aldehL 1560 [C;X3;$(C(=O)[#1])] [$([C;X4])] -!@
#
################################################
# ACETALS
# cut C/Ar-|-C(-O-R/Ar)(-O-R/Ar)-R/Ar
#CTR acetalK 1600 [C;X4;$(C(O-[C,c])(O-[C,c])([C,c])[C,c])] [$([C,c]C(O-[C,c])(O-[C,c]))] -!@
#CTR acetalA 1610 [C;X4;$(C(O-[C,c])(O-[C,c])([C,c])[#1])] [$([C,c]C(O-[C,c])(O-[C,c]))] -!@
#
